  "scripts": {
    "start": "cross-env NODE_ENV=dev supervisor -i server/views -w server,components -e node,js,html server.babel",
    "build": "webpack",
    "production": "webpack --config webpack.production.config.js -p & cross-env NODE_ENV=production node server.babel",
    "release": "babel components/wrapper -d components/wrapper.release && webpack --config webpack.production.config.js -p && gulp && babel server -d server.release",
    "html": "curl http://localhost:8080/webapp/skeleton/static/htmlGenerate?release=true",
    "clean": "rm -fr components/*.release && rm -fr dest/views && rm -fr dest/public/* && rm -fr public/hybrid && rm -fr public/page && rm -fr server.release"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
